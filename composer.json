{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url" : "https://github.com/roots/soil.git"
    },
    {
        "type": "package",
        "package": {
            "name": "deliciousbrains/wp-migrate-db-pro",
            "type": "wordpress-plugin",
            "version": "1.7.2",
            "dist": {
                "type": "zip",
                "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-latest.zip?licence_key=ec6ae92d-7559-468b-9dc3-ca1bce5d2388&site_url=majacmedical.dev"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "deliciousbrains/wp-migrate-db-pro-media-files",
            "type": "wordpress-plugin",
            "version": "1.4.7",
            "dist": {
                "type": "zip",
                "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-media-files-latest.zip?licence_key=ec6ae92d-7559-468b-9dc3-ca1bce5d2388&site_url=majacmedical.dev"
            }
        }
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/homepage-control.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/woo-theme-customisation.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/product-enquiry-pro.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/storefront-mega-menus.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/storefront-title-toggle.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/storefront-parallax-hero.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/storefront-powerpack.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/wp-migrate-db-pro.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/wp-migrate-db-pro-media-files.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/custom-sidebars.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/woocommerce-products-filter.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/woocommerce-product-search.git"
    },
    {
      "type": "vcs",
      "url" : "git@bitbucket.org:mjmcmahon/wp-forms.git"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.2.0",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.9.1",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/contact-form-7": "4.9.*",
    "wpackagist-plugin/duracelltomi-google-tag-manager": "1.7.*",
    "wpackagist-plugin/woocommerce": "3.2.*",
    "wpackagist-plugin/wordfence": "6.3.6",
    "wpackagist-plugin/mailgun": "1.5.8.x",
    "wpackagist-plugin/redirection": "2.10.*",
    "wpackagist-plugin/taxonomy-terms-order": "1.5.*",
    "wpackagist-plugin/disable-comments": "1.7.*",
    "digitoimistodude/woothemes-updater": "1.7.2",
    "deliciousbrains/wp-migrate-db-pro": "1.7.2",
    "deliciousbrains/wp-migrate-db-pro-media-files": "1.4.x",
    "roots/soil": "3.7.1",
    "mjmcmahon/homepage-control": "2.0.2",
    "mjmcmahon/storefront-mega-menus": "1.4.*",
    "mjmcmahon/storefront-parallax-hero": "1.5.4",
    "mjmcmahon/storefront-powerpack": "1.4.*",
    "mjmcmahon/storefront-title-toggle": "1.2.*",
    "mjmcmahon/custom-sidebars": "3.*",
    "mjmcmahon/theme-customisations": "1.0.0",
    "mjmcmahon/product-enquiry-pro": "4.3.1",
    "mjmcmahon/woocommerce-products-filter": "4.1.0",
    "mjmcmahon/woocommerce-product-search": "1.10.2",
    "mjmcmahon/wp-forms": "1.4.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "wpackagist-plugin/reveal-template": "3.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin", "roots/soil"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  }
}
